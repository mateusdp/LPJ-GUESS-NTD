)


! Arctic specific groups and PFTs
! Values from Wolf et al. and Wania et al., unless otherwise stated.
! **********************************************************************

group "tallshrub" (

	shrub

	! Allometry
	crownarea_max 4.0
	k_allom2 6
	k_latosa 500
	longevity 100 ! Wolf et al have 40. Juniper can live for > 100 years
	rootdist 0.14 0.14 0.14 0.14 0.14 0.03 0.03 0.03 0.03 0.03 0.03 0.03 0.03 0.03 0.03
	turnover_root 0.7

	! Bioclimatic limits
	gdd5min_est 0
	gdd0_min 600
	tcmin_surv -32.5
	tcmax_est 5
	twmin_est -1000

	! stresses
	fireresist 0.12

	! Shade tolerance (Zhang et al. 2014 - Table S2b)
	est_max 0.5
	parff_min 2000000
	alphar 10.0
	greff_min 0.02
	turnover_sap 0.015
)



group "lowshrub" (
	! Typical species: Vaccinium vitis-idaea, Cassiope tet.,
	! Ledum palustre, Erica spec.

	shrub

	! Allometry
	crownarea_max 1.0
	k_allom1 10
	k_allom2 4
	k_latosa 125
	longevity 25
	rootdist 0.16 0.16 0.16 0.16 0.16 0.02 0.02 0.02 0.02 0.02 0.02 0.02 0.02 0.02 0.02	
	turnover_root 0.7

	! Bioclimatic limits
	gdd5min_est 0
	gdd0_min 250

	! Shade tolerance (Zhang et al. 2014 - Table S2b)
	est_max 0.75
	parff_min 1000000
	alphar 2.0
	greff_min 0.012
	turnover_sap 0.01

)


group "prostratedwarfshrub" (
	! Prostrate dwarf shrubs
	! Typical species: Vaccinium oxycoccus, Salix arctica, Arctostaphylos alpinus

	shrub

	! Allometry
	rootdist 0.18 0.18 0.18 0.18 0.18 0.01 0.01 0.01 0.01 0.01 0.01 0.01 0.01 0.01 0.01
	!root_beta 0.9555
	turnover_root 0.5

	! Bioclimatic limits
	tcmin_surv -1000
	tcmin_est -1000
	tcmax_est 5
	twmin_est -1000
	gdd5min_est 0
	gdd0_min 150
	gdd0_max 800 !orig is 350, temp testing 1200 for abisko. Jing, turned off for the abisko area, since it can normally over 1200.
	min_snow 20.0

	! Shade tolerance (Zhang et al. 2014 - Table S2b) - Assumed to be the same as low shrubs
	est_max 0.75
	parff_min 500000 ! Lower than the low shrub value
	alphar 2.0
	greff_min 0.012
	turnover_sap 0.01

	! Tundra peatlands
	wtp_max -200 ! mm
	inund_duration 10 ! days

	! BVOC values - Guy Schurgers, October/November 2011
	eps_iso 23.5
	seas_iso 1
	!eps_mon 1.1
	!storfrac_mon 0.5
	eps_mon 0.37 0.2 0.23 0.1 0.1 0.09 0.1 0.22 0.19 ! C3G values - must update
	storfrac_mon 0.4 0.8 0.8 0.4 0.4 0.5 0.8 0.2 0.5 ! C3G values - must update
	ga 0.030


)

group "cushionforblichenmosstundra" (

	! Cushion forb, lichen and moss tundra
	! Found in (tundra, High Arctic) peatlands
	! Typical species: Saxifragacea, Caryophyllaceae, Papaver, Draba, lichens, mosses

